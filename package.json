{
  "name": "project-boilerplate",
  "version": "0.0.0",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/project-boilerplate.git"
  },
  "scripts": {
    "build": "run-s clean patch build:gatsby",
    "build:lambda": "netlify-lambda build src/lambda --config lambda-webpack-config.js",
    "build:gatsby": "yarn build:lambda && NODE_ENV=production gatsby build",
    "clean": "run-p clean:gatsby clean:functions",
    "clean:gatsby": "gatsby clean",
    "clean:functions": "rm -rf dist-functions",
    "dev": "netlify dev --command \"yarn dev:gatsby\"",
    "dev:gatsby": "yarn patch && gatsby develop",
    "dev:functions": "netlify-lambda serve src/lambda --config lambda-webpack-config.js",
    "env": "node src/utils/env",
    "lint": "eslint ./",
    "live": "open-cli https://project-boilerplate.netlify.com/",
    "netlify": "open-cli https://app.netlify.com/sites/project-boilerplate/deploys",
    "patch": "run-p patch:cms-config patch:netlify-toml",
    "patch:cms-config": "node src/utils/build-cms-config",
    "patch:netlify-toml": "node src/utils/netlify-toml",
    "prepublishOnly": "yarn build",
    "reset": "node src/utils/reset",
    "serve": "gatsby serve --open",
    "test": "jest",
    "travis": "open-cli https://travis-ci.org/escaladesports/project-boilerplate/branches"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
